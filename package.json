{
  "name": "unplugindts-bundle-generator",
  "version": "3.0.0",
  "description": "DTS bundle generator for Unplugin",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "dts",
    "bundle",
    "typescript"
  ],
  "repository": "https://github.com/f-lawe/unplugin-dts-bundle-generator",
  "license": "MIT",
  "author": "Fran√ßois Lavaud-Wernert <francois@lavaud.family>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/dts-bundle-generator.d.ts",
      "import": "./dist/dts-bundle-generator.mjs",
      "require": "./dist/dts-bundle-generator.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    }
  },
  "main": "dist/dts-bundle-generator.cjs",
  "module": "dist/dts-bundle-generator.mjs",
  "source": "src/index.ts",
  "types": "dist/dts-bundle-generator.d.ts",
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist",
    "lint": "eslint . --fix && sort-package-json",
    "lint:ci": "eslint . && sort-package-json --check",
    "prepack": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit",
    "typecheck:ci": "tsc --noEmit",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "dts-bundle-generator": "^9.3.1",
    "unplugin": "^2.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.1",
    "@nuxt/kit": "^3.17.4",
    "@nuxt/schema": "^3.17.4",
    "@types/node": "^24.0.4",
    "eslint": "^9.12.0",
    "jiti": "^2.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "sort-package-json": "^3.3.1",
    "typescript": "^5.0.2",
    "vite": "^7.0.0",
    "webpack": "^5.99.9"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=10.0.0"
  }
}
